{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "antlr-debug",
            "request": "launch",
            "name": "Debug ANTLR4 grammar",
            "grammar": "${workspaceFolder}/src/main/python/compilador.g4",
            "input": "${workspaceFolder}/input/programa.txt",
            "startRule": "programa",
            "printParseTree": true,
            "visualParseTree": true
        },

        {
            "name": "Python: App.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/main/python/App.py",
            "args": ["${workspaceFolder}/input/programa.txt"],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/src/main/python"
        },

        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },

        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}",
        },

        {
            "type": "java",
            "name": "App",
            "request": "launch",
            "mainClass": "dhs-joaquincontreras_2025.app",
            "projectName": "DHS2025"
        }
    ]
}